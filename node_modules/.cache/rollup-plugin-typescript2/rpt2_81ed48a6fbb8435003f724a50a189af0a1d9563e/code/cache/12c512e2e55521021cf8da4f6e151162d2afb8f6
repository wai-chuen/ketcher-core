    }\r\n        ]\r\n    };\r\n}\r\n//# sourceMappingURL=simpleObjectToStruct.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/domain/entities/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/domain/serializers/ket/helpers.ts"],"map":"{\"version\":3,\"file\":\"simpleObjectToStruct.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../../src/domain/serializers/ket/fromKet/simpleObjectToStruct.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,EAAE,YAAY,EAAE,gBAAgB,EAAU,IAAI,EAAE,0BAAuB;AAC9E,OAAO,EAAE,yBAAyB,EAAE,MAAM,YAAY,CAAA;AAEtD,MAAM,UAAU,oBAAoB,CAAC,OAAY,EAAE,MAAc;IAC/D,MAAM,MAAM,GACV,OAAO,CAAC,IAAI,CAAC,IAAI,KAAK,QAAQ,CAAC,CAAC,CAAC,eAAe,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,OAAO,CAAC,IAAI,CAAA;IAC1E,MAAM,CAAC,aAAa,CAAC,GAAG,CAAC,IAAI,YAAY,CAAC,yBAAyB,CAAC,MAAM,CAAC,CAAC,CAAC,CAAA;IAC7E,OAAO,MAAM,CAAA;AACf,CAAC;AAED;;;;GAIG;AACH,SAAS,eAAe,CAAC,OAAO;IAC9B,MAAM,MAAM,GAAG,IAAI,CAAC,IAAI,CAAC,OAAO,CAAC,IAAI,CAAC,GAAG,CAAC,CAAC,CAAC,EAAE,OAAO,CAAC,IAAI,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,CAAA;IAClE,MAAM,IAAI,GAAG,OAAO,CAAC,IAAI,CAAC,GAAG,CAAC,CAAC,CAAC,CAAA;IAChC,OAAO;QACL,IAAI,EAAE,gBAAgB,CAAC,OAAO;QAC9B,GAAG,EAAE;YACH;gBACE,CAAC,EAAE,IAAI,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,MAAM,CAAC;gBAC5B,CAAC,EAAE,IAAI,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,MAAM,CAAC;gBAC5B,CAAC,EAAE,IAAI,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,MAAM,CAAC;aAC7B;YACD;gBACE,CAAC,EAAE,IAAI,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,MAAM,CAAC;gBAC5B,CAAC,EAAE,IAAI,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,MAAM,CAAC;gBAC5B,CAAC,EAAE,IAAI,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,MAAM,CAAC;aAC7B;SACF;KACF,CAAA;AACH,CAAC\"}","dts":{"name":"/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/node_modules/.cache/rollup-plugin-typescript2/placeholder/domain/serializers/ket/fromKet/simpleObjectToStruct.d.ts","writeByteOrderMark":false,"text":"/****************************************************************************\r\n * C