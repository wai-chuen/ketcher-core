        return bond[attribute] === value;\r\n        }\r\n        return false;\r\n    }\r\n    invert() {\r\n        const inverted = new BondAttr();\r\n        inverted.data = this.data2;\r\n        inverted.data2 = this.data;\r\n        return inverted;\r\n    }\r\n}\r\n//# sourceMappingURL=BondAttr.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/application/editor/operations/base.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/application/editor/operations/OperationType.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/application/render/index.ts"],"map":"{\"version\":3,\"file\":\"BondAttr.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../../src/application/editor/operations/bond/BondAttr.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,EAAE,aAAa,EAAE,MAAM,SAAS,CAAA;AACvC,OAAO,EAAE,aAAa,EAAE,MAAM,kBAAkB,CAAA;AAShD,MAAM,OAAO,QAAS,SAAQ,aAAa;IACzC,IAAI,CAAa;IACjB,KAAK,CAAa;IAElB,YAAY,MAAY,EAAE,SAAe,EAAE,KAAW;QACpD,KAAK,CAAC,aAAa,CAAC,SAAS,EAAE,CAAC,CAAC,CAAA;QACjC,IAAI,CAAC,IAAI,GAAG,EAAE,GAAG,EAAE,MAAM,EAAE,SAAS,EAAE,KAAK,EAAE,CAAA;QAC7C,IAAI,CAAC,KAAK,GAAG,IAAI,CAAA;IACnB,CAAC;IAED,OAAO,CAAC,QAAkB;QACxB,IAAI,IAAI,CAAC,IAAI,EAAE;YACb,MAAM,EAAE,SAAS,EAAE,GAAG,EAAE,KAAK,EAAE,GAAG,IAAI,CAAC,IAAI,CAAA;YAC3C,MAAM,IAAI,GAAG,QAAQ,CAAC,QAAQ,CAAC,KAAK,CAAC,GAAG,CAAC,GAAG,CAAE,CAAA;YAE9C,IAAI,CAAC,IAAI,CAAC,KAAK,EAAE;gBACf,IAAI,CAAC,KAAK,GAAG;oBACX,GAAG,EAAE,GAAG;oBACR,SAAS,EAAE,SAAS;oBACpB,KAAK,EAAE,IAAI,CAAC,SAAS,CAAC;iBACvB,CAAA;aACF;YAED,IAAI,CAAC,SAAS,CAAC,GAAG,KAAK,CAAA;YAEvB,aAAa,CAAC,cAAc,CAAC,QAAQ,EAAE,GAAG,CAAC,CAAA;YAC3C,IAAI,SAAS,KAAK,MAAM,EAAE;gBACxB,aAAa,CAAC,cAAc,CAAC,QAAQ,EAAE,GAAG,CAAC,CAAA;aAC5C;SACF;IACH,CAAC;IAED,OAAO,CAAC,QAAkB;QACxB,IAAI,IAAI,CAAC,IAAI,EAAE;YACb,MAAM,EAAE,SAAS,EAAE,GAAG,EAAE,KAAK,EAAE,GAAG,IAAI,CAAC,IAAI,CAAA;YAC3C,MAAM,IAAI,GAAG,QAAQ,CAAC,QAAQ,CAAC,KAAK,CAAC,GAAG,CAAC,GAAG,CAAE,CAAA;YAC9C,OAAO,IAAI,