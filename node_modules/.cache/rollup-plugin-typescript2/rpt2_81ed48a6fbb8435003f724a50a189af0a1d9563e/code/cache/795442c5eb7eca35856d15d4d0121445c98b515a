orsPosition extends BaseOperation {\r\n    history;\r\n    constructor(history) {\r\n        super(OperationType.RESTORE_DESCRIPTORS_POSITION);\r\n        this.history = history;\r\n    }\r\n    execute(restruct) {\r\n        const struct = restruct.molecule;\r\n        const sgroups = Array.from(struct.sgroups.values());\r\n        sgroups.forEach((sgroup) => {\r\n            sgroup.pp = this.history[sgroup.id];\r\n            struct.sgroups.set(sgroup.id, sgroup);\r\n            BaseOperation.invalidateItem(restruct, 'sgroupData', sgroup.id, 1);\r\n        });\r\n    }\r\n    invert() {\r\n        return new AlignDescriptors();\r\n    }\r\n}\r\nexport { AlignDescriptors, RestoreDescriptorsPosition };\r\n//# sourceMappingURL=descriptors.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/application/editor/operations/base.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/application/editor/operations/OperationType.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/application/render/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/src/domain/entities/index.ts"],"map":"{\"version\":3,\"file\":\"descriptors.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../src/application/editor/operations/descriptors.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,EAAE,aAAa,EAAE,MAAM,QAAQ,CAAA;AACtC,OAAO,EAAE,aAAa,EAAE,MAAM,iBAAiB,CAAA;AAE/C,OAAO,EAAE,IAAI,EAAE,iCAAuB;AAEtC,6EAA6E;AAE7E,MAAM,gBAAiB,SAAQ,aAAa;IAC1C,OAAO,CAAK;IAEZ;QACE,KAAK,CAAC,aAAa,CAAC,iBAAiB,CAAC,CAAA;QACtC,IAAI,CAAC,OAAO,GAAG,EAAE,CAAA;IACnB,CAAC;IAED,OAAO,CAAC,QAAkB;QACxB,MAAM,MAAM,GAAG,QAAQ,CAAC,QAAQ,CAAA;QAChC,MAAM,OAAO,GAAU,KAAK,CAAC,IAAI,CAAC,MAAM,CAAC,OAAO,CAAC,MAAM,EAAE,CAAC,CAAC,OAAO,EAAE,CAAA;QAEpE,MAAM,SAAS,GAAQ,MAAM,CAAC,sBAAsB,EAAE,CAAA;QACtD,IAAI,UAAU,GAAG,IAAI,IAAI,CAAC,SAAS,CAAC,GAAG,CAAC,CAAC,EAAE,SAAS,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,GAAG,